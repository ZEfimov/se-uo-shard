use uo;
include ":attributes:attributes";

program fixSkills(who)
   var chardata := GetObjProperty(who, "chardata");
   if (!chardata)
      return;
   endif

	foreach skill in (chardata.skills)
      skill := StrReplace(skill, " ", "_" );
		AP_SetTrueSkill(who, skill, 1);
      sleepms(2);
	endforeach
endprogram