use uo;
use os;
include ":yggdrasil:spawnSpot";
include ":yggdrasil:treasures";
include "include/dungeons";

program Creating(nuvem)
   var heigth := GetWorldHeight(nuvem.x, nuvem.y);
   var mov := MoveObjectToLocation(nuvem, nuvem.x, nuvem.y, heigth+20, nuvem.realm, MOVEOBJECT_FORCELOCATION|MOVEITEM_IGNOREMOVABLE);
   sleepms(2);
endprogram