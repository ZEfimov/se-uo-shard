use uo;
use os;

program canRemove(mobile, container, item, move_type)
	if ( mobile.cmdlevel )
		return 1;
	elseif ( move_type == MOVETYPE_COREMOVE )
		return 1;
	else
		PrintTextAbovePrivate(container, "Não pode remover itens desse container.", mobile);
		return 0;
	endif
endprogram
