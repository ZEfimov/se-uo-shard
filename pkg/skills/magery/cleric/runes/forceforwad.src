use uo;
use os;

include "include/utils";
include "include/sysEvent";

program ForceForward(mobile)
   if (!mobile.my_template() || !mobile.my_template()["moriquendi"])
      return;
   endif

   PushBack(mobile, 2, 300, 1);
   var ev := struct{
		type := EVID_PEACEMADE
   };
   SendEvent(mobile, ev);
endprogram