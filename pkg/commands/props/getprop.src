use uo;
include "include/say";

program propertyinfo(who, property)
	var targ := target(who);
	if ( !targ )
		SendSysMessage(who, "Cancelled");
		return;
	endif
	SendSysMessageEx(who, "Property "+property+": "+targ.get_member(property));
endprogram
