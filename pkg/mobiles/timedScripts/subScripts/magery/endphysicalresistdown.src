 
use uo;
use os;

include ":attributes:attributes";
include ":gumps:yesno";

program TimedScript_Clumsy(params)
	var mobile := params[1];

	if (GetObjProperty(mobile, "phDownMod") != error)
		var mod := GetObjProperty(mobile, "phDownMod");
		EraseObjProperty(mobile, "phDownMod");
		mobile.resist_physical_mod := mobile.resist_physical_mod + mod;
	endif
	return 1;
endprogram

