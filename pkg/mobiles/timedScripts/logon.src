use uo;
use os;

include ":timedscripts:timedScripts";


program Logon(who)
	var result := TS_StartController(who);
	if ( result.errortext )
		SendSysMessage(who, "->"+result.errortext);
	endif

   if (who.dead && !TS_GetTimer(who, ))

	return 1;
endprogram

