
use uo;
use basic;

program Install()
    return 1;
endprogram

exported function my_template(pet)
	var npctemplate := SplitWords(pet.npctemplate, ":");
   return npctemplate[3];
endfunction

exported function package(pet)
	var npctemplate := SplitWords(pet.npctemplate, ":");
   return npctemplate[2];
endfunction